  <SQLQuery>
    <![CDATA[SELECT
bpo.BPO_ID ,
bpo.bpo BPO,
ph.style,
ph.season,
pl.COLOR Color,
pl.wash_name wash,
l.wash_type,
h.RECEIVE_NO,
l.QUANTITY QUANTITY,
l.PRODUCTION_TYPE

  FROM MNJ_MFG_ISSUE_REC_WASH_H h, MNJ_MFG_ISSUE_REC_WASH_L_H lh, 
  MNJ_MFG_ISSUE_REC_WASH_L l,XX_OM_POC_H_T ph,  XX_OM_POC_L_T pl, 
  XX_OM_BPO_INFO_T bpo
 WHERE h.header_id = lh.header_id
  and lh.line_id = l.header_id
  and l.bpo_id=bpo.bpo_id
  and bpo.fob_id=pl.fob_id
  and pl.poc_id=ph.poc_id]]>
  </SQLQuery>
