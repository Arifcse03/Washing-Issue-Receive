  <SQLQuery>
    <![CDATA[/*Formatted on 3/4/2021 4:21:44 PM (QP5 v5.256.13226.35538) */
select * from(
SELECT OOBA.BPO_ID,
OOBA.BPO,
OOHA.BUYER_ID,
OOHA.SEASON,
OOHA.STYLE,
OOLA.COLOR,
OOLA.WASH_NAME,
OOHA.LC_UNIT,
L.PRODUCTION_TYPE,
NVL(l.ATTRIBUTE4,0)
- NVL (
(SELECT SUM (NVL (MNJ_ISSUE_REC_WASH_DEL_L_EO.QUANTITY, 0))
FROM MNJ_ISSUE_REC_WASH_DEL_L MNJ_ISSUE_REC_WASH_DEL_L_EO
WHERE MNJ_ISSUE_REC_WASH_DEL_L_EO.BPO_ID = OOBA.bpo_ID
AND MNJ_ISSUE_REC_WASH_DEL_L_EO.SYSTEM_ID =
OOLA.SYSTEM_ID),
0)
QUANTITY,
NVL (
(SELECT SUM (NVL (MNJ_ISSUE_REC_WASH_DEL_L_EO.QUANTITY, 0))
FROM MNJ_ISSUE_REC_WASH_DEL_L MNJ_ISSUE_REC_WASH_DEL_L_EO
WHERE MNJ_ISSUE_REC_WASH_DEL_L_EO.BPO_ID = OOBA.bpo_ID
AND MNJ_ISSUE_REC_WASH_DEL_L_EO.SYSTEM_ID = OOLA.SYSTEM_ID),
0)
ISSUE_QTY,
OOLA.System_Id
FROM mnj_mfg_issue_rec_wash_h h,
mnj_issue_rec_wash_rec_l l,
xx_om_poc_h_t OOHA,
xx_om_poc_l_t OOLA,
xx_om_bpo_info_t OOBA
WHERE h.header_id = l.header_id
AND OOHA.POC_ID = H.POC_ID
AND OOHA.POC_ID = OOLA.POC_ID
AND OOLA.FOB_ID = OOBA.FOB_ID
AND L.BPO_ID = OOBA.BPO_ID) where QUANTITY>0]]>
  </SQLQuery>
